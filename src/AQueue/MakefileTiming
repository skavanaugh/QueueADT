BUILD = ../../build
CPP = g++
CPPFLAGS = -Wall -Wextra
TTEST = ../TimingTests

$(BUILD)/Atime: $(TTEST)/queue_tester.cpp Queue.o
	$(CPP) $(CPPFLAGS) -o $(BUILD)/Atime $(TTEST)/queue_tester.cpp Queue.o

Queue.o: Queue.h Queue.cpp 
	$(CPP) $(CPPFLAGS) -c Queue.cpp

clean:
	rm -f *.o
	rm -f *~
	rm -f $(BUILD)/Atime
